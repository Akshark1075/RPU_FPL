<% include partials/header.ejs%>
<div class="container">
  
  <table class="table  well" id="table" style="margin-top: 15px;color:rgb(230, 233, 239);background:rgb(27, 28, 28);


   width:70%;margin:0 auto;text-align: center;">
   <!--  background: #02111D;  
background: -webkit-linear-gradient(to right, #02111D, #037BB5, #02111D); 
background: linear-gradient(to right, #02111D, #037BB5, #02111D);-->
  <thead>
    <tr>
      <th style="text-align: center;" scope="col">####</th>
      <th style="text-align: center;" scope="col">User_Name</th>
      <th style="text-align: center;" scope="col">Team_Name</th>
      <th style="text-align: center;" scope="col">Points</th>
        <th style="text-align: center;" scope="col">Total</th>
       
    </tr>
  </thead>
  <tbody>
    
    
  

      <%user.forEach(function(userr){  %>

    
    <tr>
      
      <th> #</th>
      <td><%= userr.username%></td>
     <% if(userr.team[0]){%>
      <td> <a href="/showpage/<%=userr.team[0]._id %>" class="btn btn-md btn-info"><%= userr.team[0].Teamname%> </a></td>
      
      <% }else{ %>
      <td>Unknown FC</td>
      <% }%>
      
       <% if(userr.team[0]){%>
      <td><%= userr.team[0].Score%></td>
       
      <% }else{ %>
      <td>NA</td>
      <% }%>
      <%if(userr.Total!=null){ %>
     <td><%=userr.Total %></td>
     <% }else{ %>
      <td>NA</td>
      <% }%>
      </tr>
       
  <%   })%> 
    
 
      
      
      
      
      
      
      
      
  </tbody>
  
  
  
  
</table>
</div>







<% include partials/footer.ejs%>